{
  "name": "@asecs/frontpage",
  "version": "2.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "svelte-kit sync",
    "prebuild": "pnpm --filter '@agapi/ui' build",
    "build": "vite build --mode prod",
    "build:api": "rollup -c",
    "prepare": "panda codegen",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "docker:build": "cp ~/.npmrc ./ && docker build . -f Dockerfile -t $npm_package_config_image --no-cache --platform linux/amd64/8",
    "pipeline:deploy:dev": "npm run build -- --mode dev && NODE_ENV=dev cdk deploy --all --force --require-approval never",
    "pipeline:deploy:stage": "npm run build -- --mode stage && NODE_ENV=stage cdk deploy --all --force --require-approval never",
    "pipeline:deploy:prod": "npm run build -- --mode prod && NODE_ENV=prod cdk deploy --all --force --require-approval never",
    "deploy:dev": "npm run build -- --mode dev && NODE_ENV=dev cdk deploy --all --force --require-approval never --profile asecs-dev",
    "deploy:stage": "npm run build -- --mode stage && NODE_ENV=stage cdk deploy --all --force --require-approval never --profile asecs-stage",
    "deploy:prod:force": "npm run build -- --mode prod && NODE_ENV=prod cdk deploy --all --force --require-approval never --profile asecs-prod",
    "start": "AWS_PROFILE=asecs-dev vite dev --mode dev",
    "start:stage": "AWS_PROFILE=asecs-stage vite dev --mode stage",
    "preview": "vite preview",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@agapi-development-tools/eslint-config": "workspace:*",
    "@types/react": "*",
    "@melt-ui/pp": "0.3.2",
    "@pandacss/dev": "1.4.2",
    "@pandacss/preset-panda": "1.4.2",
    "@playwright/test": "1.56.0",
    "@rollup/plugin-alias": "5.1.1",
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "16.0.2",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.1.4",
    "@sveltejs/kit": "2.46.5",
    "@sveltejs/vite-plugin-svelte": "6.2.1",
    "@types/node": "22.12.0",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "autoprefixer": "10.4.21",
    "aws-cdk-lib": "2.219.0",
    "aws-sdk": "2.1692.0",
    "constructs": "10.4.2",
    "esbuild": "0.25.10",
    "eslint": "9.38.0",
    "marked": "16.4.0",
    "monaco-editor": "^0.45.0",
    "postcss": "8.5.6",
    "source-map-support": "0.5.21",
    "svelte": "5.40.0",
    "svelte-check": "4.3.3",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "vite": "7.1.9",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-mkcert": "1.17.9",
    "vitest": "3.2.4",
    "stream-throttle": "0.1.3",
    "@types/stream-throttle": "0.1.4",
    "jiti": "2.6.1"
  },
  "dependencies": {
    "@asecs/shared": "workspace:*",
    "@agapi-development-tools/rollup-monorepo-plugin": "workspace:*",
    "@agapi-development-tools/sveltekit-aws-adapter": "workspace:*",
    "@agapi-development-tools/vite-sveltekit-env": "workspace:*",
    "@agapi-development-tools/parachute": "workspace:*",
    "@agapi-development-tools/common-utilities": "workspace:*",
    "@agapi/ui": "workspace:*",
    "@payloadcms/db-mongodb": "3.62.1",
    "@types/aws-lambda": "8.10.146",
    "@types/cookie": "1.0.0",
    "@zag-js/svelte": "1.26.1",
    "phosphor-svelte": "workspace:*",
    "acorn": "8.15.0",
    "acorn-walk": "8.3.4",
    "aws-cdk": "2.1030.0",
    "cookie": "1.0.2",
    "dotenv": "17.2.3",
    "glob": "11.0.3",
    "lambda-warmer": "2.3.0",
    "node-fetch": "3.3.2",
    "rollup": "4.52.4",
    "payload": "3.62.1",
    "source-map-support": "0.5.21",
    "tsx": "4.20.6",
    "yaml": "2.8.1"
  }
}
