{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": false,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": false,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "types": [
      "node"
    ]
    // Path aliases are handled by https://kit.svelte.dev/docs/configuration#alias
  }
}