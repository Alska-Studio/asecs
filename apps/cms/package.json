{
  "name": "@asecs/cms",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "ESLINT_FLAGS=\"v10_config_lookup_from_file\" APP_ENV=dev cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build && npx @agapi-development-tools/nextjs-aws-adapter pack",
    "pipeline:deploy:dev": "npm run build:dev && NODE_ENV=dev tsx ./stack/index.ts",
    "pipeline:deploy:stage": "npm run build:stage && NODE_ENV=stage tsx ./stack/index.ts",
    "pipeline:deploy:prod": "npm run build:prod && NODE_ENV=prod cdk deploy --all --force --require-approval never",
    "deploy:dev": "npm run build:dev && NODE_ENV=dev tsx ./stack/index.ts",
    "deploy:stage": "npm run build:stage && NODE_ENV=stage tsx ./stack/index.ts",
    "deploy:prod:force": "npm run build:prod && NODE_ENV=prod tsx ./stack/index.ts",
    "start": "cross-env NODE_OPTIONS=--no-deprecation AWS_PROFILE=asecs-dev next dev",
    "start:safe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "start:prod": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation AWS_PROFILE=asecs-dev payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation AWS_PROFILE=asecs-dev payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "seed:dev": "cross-env AWS_PROFILE=asecs-dev tsx ./seed/index.ts"
  },
  "dependencies": {
    "@asecs/shared": "1.0.0",
    "@next/bundle-analyzer": "15.4.4",
    "@payloadcms/db-mongodb": "3.62.1",
    "@payloadcms/next": "3.62.1",
    "@payloadcms/payload-cloud": "3.62.1",
    "@payloadcms/richtext-lexical": "3.62.1",
    "@payloadcms/ui": "3.62.1",
    "cross-env": "7.0.3",
    "dotenv": "17.2.3",
    "graphql": "16.12.0",
    "next": "15.5.6",
    "payload": "3.62.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "serverless-http": "4.0.0",
    "lambda-warmer": "2.3.0"
  },
  "devDependencies": {
    "@agapi-development-tools/eslint-config": "workspace:*",
    "@agapi-development-tools/nextjs-aws-adapter": "workspace:*",
    "@aws-sdk/credential-providers": "3.919.0",
    "@payloadcms/db-mongodb": "3.62.1",
    "@payloadcms/email-nodemailer": "3.62.1",
    "@payloadcms/next": "3.62.1",
    "@payloadcms/richtext-lexical": "3.62.1",
    "@payloadcms/translations": "3.62.1",
    "@payloadcms/payload-cloud": "3.62.1",
    "@payloadcms/storage-s3": "3.62.1",
    "@payloadcms/plugin-cloud-storage": "3.62.1",
    "@payloadcms/ui": "3.62.1",
    "@types/node": "22.12.0",
    "@types/react": "*",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.2",
    "@eslint/eslintrc": "3.3.1",
    "aws-cdk": "2.1031.1",
    "aws-cdk-lib": "2.221.0",
    "envalid": "8.1.0",
    "eslint": "9.38.0",
    "eslint-config-next": "15.4.4",
    "jsdom": "27.0.0",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "5.1.4",
    "webpack": "5.98.0",
    "jiti": "2.6.1"
  }
}