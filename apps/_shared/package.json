{
  "name": "@asecs/shared",
  "version": "1.0.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*/index.ts",
      "default": "./src/*/index.ts"
    },
    "./payload-config": {
      "default": "./src/payload/index.ts"
    },
    "./payload-types": {
      "default": "./src/payload/payload-types.ts"
    }
  },
  "scripts": {
    "generate:types": "AWS_PROFILE=asecs-dev payload generate:types"
  },
  "dependencies": {
    "@agapi/ui": "workspace:*",
    "@aws-sdk/credential-provider-node": "3.925.0",
    "@aws-sdk/client-sts": "3.925.0",
    "@aws-sdk/client-ssm": "3.925.0",
    "@aws-sdk/client-s3": "3.925.0",
    "@payloadcms/db-mongodb": "3.62.1",
    "@payloadcms/email-nodemailer": "3.62.1",
    "@payloadcms/next": "3.62.1",
    "@payloadcms/richtext-lexical": "3.62.1",
    "@payloadcms/translations": "3.62.1",
    "@payloadcms/payload-cloud": "3.62.1",
    "@payloadcms/storage-s3": "3.62.1",
    "@payloadcms/plugin-cloud-storage": "3.62.1",
    "@payloadcms/ui": "3.62.1",
    "payload": "3.62.1",
    "sharp": "0.34.4"
  },
  "devDependencies": {
    "typescript": "5.9.3",
    "eslint": "9.38.0"
  }
}
